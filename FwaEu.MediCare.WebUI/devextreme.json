{
	"applicationEngine": "vue",
	"vue": {
		"version": 3
	},
	"build": {
		"commands": [
			{
				"command": "build-theme",
				"options": {
					"inputFile": "src/Fwamework/DevExtreme/Themes/metadata.base.json",
					"outputFile": "src/Fwamework/DevExtreme/Themes/generated/theme.base.css"
				}
			},
			{
				"command": "export-theme-vars",
				"options": {
					"inputFile": "src/Fwamework/DevExtreme/Themes/metadata.base.json",
					"outputFile": "src/Fwamework/DevExtreme/Themes/generated/variables.base.scss"
				}
			}
		]
	}
}